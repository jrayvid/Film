<h1>Edit Your Account</h1>

<div id="editform">
<%= form_for @user do |f| %>
  <%= f.label :Firstname %><br>
  <%= f.text_field :fname %><br><br>
  <%= f.label :Lastname %><br>
  <%= f.text_field :lname %><br><br>
  <%= f.label :City %><br>
  <%= f.text_field :city %><br><br>
  <%= f.label :State %><br>
  <%= f.text_field :state %><br><br>
  <%= f.label :Email %><br>
  <%= f.text_field :email %><br><br>
  <%= f.label :username %><br>
  <%= f.text_field :username %><br><br>
  <%= f.label :password %><br>
  <%= f.password_field :password %>
  <br><br>
  <%= f.submit 'Edit'%>
<% end %>
</div>
<%= render 'layouts/usernavigation' %>