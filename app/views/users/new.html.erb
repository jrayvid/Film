<h1>Sign Up!</h1>

<div id="signupform">
<%= form_for @user do |f| %>
  <%= f.label :Firstname %>
  <%= f.text_field :fname %><br><br>
  <%= f.label :Lastname %>
  <%= f.text_field :lname %><br><br>
  <%= f.label :City %>
  <%= f.text_field :city %><br><br>
  <%= f.label :State %>
  <%= f.text_field :state %><br><br>
  <%= f.label :Email %>
  <%= f.text_field :email %><br><br>
  <%= f.label :Username %>
  <%= f.text_field :username %><br><br>
  <%= f.label :Password %>
  <%= f.password_field :password %>
  <br><br>
  <%= f.submit 'Sign Up'%>
<% end %>
</div>