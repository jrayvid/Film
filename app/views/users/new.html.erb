<body class="signupbod">
  <nav id="supf">
    <ul>
    <li><%= link_to 'Home', root_path, style: 'color:#ffffff' %></li>
    <li><%=link_to 'About', about_path, style: 'color:#ffffff' %></li>
    <li><%= link_to 'Contact', contact_path, style: 'color:#ffffff' %></li>
    </ul>
  </nav>
  <div class="container center_div">
<div id="signupform">
  <h2>Sign Up!</h2>
  <%= form_for @user do |f| %>
  <h2><br>
  <%= f.text_field :fname, :placeholder => "First Name" %><br>
  <br>
  <%= f.text_field :lname, :placeholder => "Last Name" %><br>
  <br>
  <%= f.text_field :city, :placeholder => "City" %><br>
  <br>
  <%= f.text_field :state, :placeholder => "State" %><br>
  <br>
  <%= f.text_field :email, :placeholder => "Email" %><br>
  <br>
  <%= f.text_field :username, :placeholder => "UserName" %><br>
  <br>
  <%= f.password_field :password, :placeholder => "Password" %>
  <br>
  <%= f.submit 'Sign Up'%></h2>
<% end %>
</div>
</div>
</body>

