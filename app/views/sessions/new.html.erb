<body>
<img src= "/assets/clapper.png">

<div class="img"> 
<h2>Log In</h2>
<h2><%= flash[:notice] if flash[:notice] %></h2>
	<%= form_tag login_path do%>
		<%= label_tag :username %>
		<%= text_field_tag :username %>
		<%= label_tag :password %>
		<%= text_field_tag :password %>
		<%= submit_tag "Log In" %>
	<% end -%>	
</div>

</body>