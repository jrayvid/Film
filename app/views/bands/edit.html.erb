

<%= form_for @band do |f| %>
  <%= f.label :'Band Name' %><br>
  <%= f.text_field :bname %><br>
  <%= f.label :City %><br>
  <%= f.text_field :city %><br>
  <%= f.label :State %><br>
  <%= f.text_field :state %><br>
  <%= f.label :Email %><br>
  <%= f.text_field :email %><br>
  <%= f.label :HeadShot %><br>
  <%= f.file_field :avatar%><br>
  <%= f.submit 'Edit'%>
<% end %>