

<%= form_for @band do |f| %>
  <h3><%= f.label :'Band Name' %><br>
  <%= f.text_field :bname %><br>
  <%= f.label :City %><br>
  <%= f.text_field :city %><br>
  <%= f.label :State %><br>
  <%= f.text_field :state %><br>
  <%= f.label :Email %><br>
  <%= f.text_field :email %><br>
  <%= f.label :Genre %><br>
  <%= f.text_field :genre %><br>
  <%= f.label :Bio %><br>
  <%= f.text_area :bio %> <br>
  <%= f.label :HeadShot %><br>
  <%= f.file_field :avatar%><br>
  <%= f.label :mp3 %><br>
  <%= f.file_field :mp3%><br>
  <%= f.label :"Track Name"  %><br>
  <%= f.text_field :track %><br>
  <%= f.label :"Influences"  %><br>
  <%= f.text_field :influences %><br></h3>
  
  
  <h1><%= f.submit 'EDIT'%><br></h1>
  
<%= link_to "Delete Band", band_path(@band), method: :delete, data: { confirm: "You Want To Delete This Band" } %>

<% end %>